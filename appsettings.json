{
  "SeriLog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContent",
      "WithMachineName",
      "WithProcessID",
      "WithThreadID"
    ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Errors\\logs.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Default": "Server=;Database=;User Id=;Password=;"
  },
  "SMTP": {
    "From": "",
    "Host": "",
    "Username": "",
    "Password": "",
    "Port": ""
  },
  "AllowedHosts": "*"
}
